<div class="pages-header">
  <%= render "components/navbar" %>
    <h3 class="pages-title align-center text-shadow text-focus-in">ABOUT NINA</h3>
</div>

<div class="specifics row">

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">LENGTH</h4>
      <p class="details">28m (92ft)</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">BUILT</h4>
      <p class="details">1964 (refitted 2019/2020)</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">CREW</h4>
      <p class="details">4</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">GUESTS</h4>
      <p class="details">7</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">GUESTS CABIN</h4>
      <p class="details">4</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">CABIN TYPES</h4>
      <p class="details">1 × double, 2 × twin, 1 x single. All with ensuite</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">MAX SPEED</h4>
      <p class="details">17 knots</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">CRUISING SPEED</h4>
      <p class="details">11 knots</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">FUEL CONSUMPTION</h4>
      <p class="details">70 l/h at cruising speed</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">ENGINES</h4>
      <p class="details">2 aifo 740 hp</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">WATER SPORTS</h4>
      <p class="details">2 x paddleboards</p>
    </div>
  </div>

  <div class="col-12 col-lg-4">
    <div class="specific" data-aos="zoom-in">
      <h4 class="title">ENTERTAINMENT</h4>
      <p class="details">WiFi, cable TV (SKY), Netflix</p>
    </div>
  </div>

</div>

<script>
  AOS.init({
  duration: 1200,
})
</script>
